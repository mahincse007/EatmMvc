@model Eatm.Models.Bankac
@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>User Login</h2>
@using (Html.BeginForm("CheakLogin", "Transaction"))
{
    <div class="form-group">
        @Html.LabelFor(m => Model.CardNo)
        @Html.TextBoxFor(m => Model.CardNo, new{@class ="form-control"} )
    </div>

    <div class="form-group">
        @Html.LabelFor(m => Model.PIN)
        @Html.TextBoxFor(m => Model.PIN, new { @class = "form-control" })
    </div>

    <button class="btn btn-primary">Login</button>
}


